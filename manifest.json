{
    "name": "ProtoShares Address Lookup",
    "version": "0.2",
    "manifest_version": 2,

    "description": "Lookup ProtoShares addresses from context menu.",
    "icons": {
	"16": "images/protoshares-16.png",
	"48": "images/protoshares-48.png",
	"128": "images/protoshares-128.png"
    },

    "background": {
	"persistent": false,
	"scripts": [ "background.js" ]
    },
    "permissions": [
	"contextMenus"
    ]
}
